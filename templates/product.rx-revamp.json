{
  "sections": {
    "main": {
      "type": "rx-main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "dropdown"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
            "show_add_to_cart_button": true
          }
        },
        "e9848660-1c8b-4f0f-b8c1-c38dce78f164": {
          "type": "shopify:\/\/apps\/recharge-subscriptions\/blocks\/subscription-widget\/371eed76-0b44-4869-9813-730372ea378e",
          "settings": {
            "product": "{{product}}"
          }
        },
        "880a5ead-be28-4912-b9d7-5a90d320fa21": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<div class=\"product__tax caption rte\"><a href=\"\/policies\/shipping-policy\">Shipping<\/a> calculated at checkout.\n<\/div>"
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "caption": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "c50958db-6d62-42c8-8a47-729c390a856f": {
          "type": "rx_product_tab",
          "settings": {
            "ingredients_content": "<p>{{ product.metafields.my_fields._1_badge_text_4.value }}<\/p>",
            "instructions_content": "<p>{{ product.metafields.my_fields._1_badge_text_3.value }}<\/p>",
            "shipping_content": "<p>Standard Shipping 3-5 business days<\/p>"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "price",
        "variant_picker",
        "quantity_selector",
        "e9848660-1c8b-4f0f-b8c1-c38dce78f164",
        "880a5ead-be28-4912-b9d7-5a90d320fa21",
        "description",
        "caption",
        "c50958db-6d62-42c8-8a47-729c390a856f"
      ],
      "settings": {
        "enable_sticky_info": false,
        "media_size": "small",
        "constrain_to_viewport": true,
        "media_fit": "contain",
        "gallery_layout": "thumbnail_slider",
        "media_position": "left",
        "image_zoom": "lightbox",
        "mobile_thumbnails": "show",
        "hide_variants": false,
        "enable_video_looping": false,
        "padding_top": 24,
        "padding_bottom": 60
      }
    },
    "165394107800f68426": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<!--Stamped - Begin Main Widget -->\n<div \nid=\"stamped-main-widget\"   \ndata-product-id=\"{{ product.id }}\"   \ndata-name=\"{{ product.title | escape }}\"   \ndata-url=\"{{ shop.url }}{{ product.url }}\"   \ndata-image-url=\"{{ product.featured_image | product_img_url: \"large\" |replace: '?', '%3F' | replace: '&','%26'}}\" \ndata-description=\"{{ product.description | escape }}\"   \ndata-product-sku=\"{{product.handle}}\"\ndata-product-title=\"{{product.title}}\" \ndata-product-type=\"{{product.type}}\">   \n{{product.metafields.stamped.reviews}}   \n<\/div>\n<!-- Stamped - End Main Widget -->",
        "color_scheme": "background-1",
        "padding_top": 40,
        "padding_bottom": 52
      }
    },
    "related-products": {
      "type": "related-products",
      "settings": {
        "heading": "You may also like",
        "heading_size": "h2",
        "products_to_show": 4,
        "columns_desktop": 4,
        "color_scheme": "background-1",
        "image_ratio": "adapt",
        "show_secondary_image": false,
        "show_vendor": false,
        "show_rating": false,
        "columns_mobile": "2",
        "padding_top": 36,
        "padding_bottom": 24
      }
    }
