    "f60fcd36-95e3-42dc-83ea-518aef727035": {
      "type": "rx-rich-text",
      "blocks": {
        "template--14898486247489__f60fcd36-95e3-42dc-83ea-518aef727035-168139761018b48100-0": {
          "type": "heading",
          "settings": {
            "heading": "<p>A Holistic Approach to Veterinary Medicine<\/p>"
          }
        },
        "template--14898486247489__f60fcd36-95e3-42dc-83ea-518aef727035-168139761018b48100-1": {
          "type": "text",
          "settings": {
            "text": "<p>My goal is to revolutionize pet care and change the way we approach animal health. With my expertise and compassionate approach, I am dedicated to helping all of my furry patients live their best lives.<\/p>"
          }
        }
      },
      "block_order": [
