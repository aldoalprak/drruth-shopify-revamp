    "96886d29-1864-4c1d-ac95-982a65607c36": {
      "type": "image-with-text",
      "blocks": {
        "template--14862046068801__96886d29-1864-4c1d-ac95-982a65607c36-16732860917073dd0d-0": {
          "type": "heading",
          "settings": {
