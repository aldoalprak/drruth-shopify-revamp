    "4757daba-bf88-4168-8ec3-278701d02b56": {
      "type": "after_program",
      "blocks": {
        "f4f7e819-b6ed-475b-8375-53027b24b976": {
          "type": "collection",
          "settings": {
            "aftertext": "Dr. Ruth Referral Program",
            "afterdesc": "After graduation, you'll have the opportunity to be listed on our consultants’ page and  join a network of other Certified Holistic Pet Health Coaches. This program connects certified holistic pet health coaches with potential clients who are seeking high-quality, personalized care for their pets. By joining the Dr. Ruth Referral Program, you'll be listed on a dedicated page, giving you exposure to a wide audience of pet owners who are actively seeking holistic pet care services."
          }
        },
        "d8d9e195-a5bd-4b4b-b033-7cf0e68bd9ae": {
          "type": "collection",
          "settings": {
            "aftertext": "Dr. Ruth Live Q&A Sessions",
            "afterdesc": "We follow up with graduates on a weekly basis via live Q&A sessions run by Dr. Ruth. All graduates will have free access to the Q&A sessions for a year after graduation ensuring that graduates have ample time to take advantage of this resource and continue to refine their knowledge and skillsThe Q&A sessions are a great way for graduates of the program to stay connected and continue to learn and develop their skills as Certified Holistic Pet Health Coaches. "
          }
        }
      },
